<Window x:Class="PurchaseList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PurchaseList"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,0.4,-176.2">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" FontWeight="Bold" FontSize="20" Content="MoneyKeeper" HorizontalAlignment="Left" Margin="180,10,0,0" VerticalAlignment="Top" Width="165"/>
        <Button x:Name="button" Content="Log In" HorizontalAlignment="Left" Margin="180,140,0,0" VerticalAlignment="Top" Width="140"/>
        <Button x:Name="button_Copy" Content="Register" HorizontalAlignment="Left" Margin="180,176,0,0" VerticalAlignment="Top" Width="140" Click="button_Copy_Click"/>

    </Grid>
</Window>
